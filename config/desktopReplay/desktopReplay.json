{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "custom",
      "alias": "100"
    },
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "cacheClearRaw": true,
    "viewPort": "1920x1080",
    "videoParams": {
      "thumbsize": 800
    },
    "chrome": {
      "cleanUserDataDir": true,
      "collectConsoleLog": true,
      "includeResponseBodies": "all",
      "enableTraceScreenshots": true,
      "traceCategory": ["disabled-by-default-v8.cpu_profiler"],
      "args": [
        "--ignore-certificate-errors-spki-list=PhrPvGIaAMmd29hj8BCZOq096yj7uMpRNHpn5PDxI6I=",
        "--user-data-dir=/tmp/google-chrome-wpr",
        "--disable-features=PaintHoldingCrossOrigin"
      ]
    },
    "firefox": {
      "acceptInsecureCerts": true
    }
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 5000,
  "pageCompleteCheckPollTimeout": 1000,
  "cpu": true,
  "utc": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty", "coach"]
  },
  "slug": "webpagereplay",
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "annotationRetentionMinutes": 60,
    "addSlugToKey": true,
    "messages": ["pageSummary", "run"]
  },
  "copyLatestFilesToBaseGraphiteNamespace": true
}
