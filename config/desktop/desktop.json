{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "chrome": {
      "collectConsoleLog": true,
      "enableTraceScreenshots": true,
      "includeResponseBodies": "all",
      "args": [
        "--disable-features=PaintHoldingCrossOrigin"
      ]
    },
    "videoParams": {
      "framerate": 30
    },
    "firefox": {
      "includeResponseBodies": "all",
      "disableBrowsertimeExtension": true,
      "mozillaProPreferences": true
    },
    "viewPort": "1920x1080"
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 5000,
  "pageCompleteCheckPollTimeout": 1000,
  "timeToSettle": 1000,
  "cpu": true,
  "utc": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty"]
  },
  "slug": "firstView",
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "annotationRetentionMinutes": 60,
    "skipSummary": true,
    "addSlugToKey": true
  }
}
