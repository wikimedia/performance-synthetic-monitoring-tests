{
  "extends": "/config/secrets.json",
  "slug": "firstView",
  "webpagetest": {
    "median": "SpeedIndex",
    "medianrun": "fastest",
    "timeline": true,
    "first": true,
    "runs": 5,
    "connectivity": "3GFast",
    "host": "wpt.wmftest.org",
    "location": "us-east:Chrome",
    "emulateMobile": true,
    "timeout": 2400,
    "bodies": true,
    "private": false,
    "cmdline": "--disable-features=PaintHoldingCrossOrigin"
  },
  "plugins": {
    "remove": "browsertime"
  },
  "gzipHAR": true,
  "utc": true,
  "html": {
    "fetchHARFiles": true
  },
   "graphite": {
    "namespace": "sitespeed_io.webpagetestEmulatedMobile",
    "annotationRetentionMinutes": 60,
    "addSlugToKey": true
  }
}
