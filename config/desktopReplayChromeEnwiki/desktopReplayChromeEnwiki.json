{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "custom",
      "alias": "0"
    },
    "visualElements": true,
    "iterations": 21,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "cacheClearRaw": true,
    "viewPort": "1920x1080",
    "chrome": {
      "cleanUserDataDir": true,
      "collectConsoleLog": true,
      "includeResponseBodies": "all",
      "enableTraceScreenshots": true,
      "trace": true,
      "traceCategories": "-*,disabled-by-default-lighthouse,v8,v8.execute,blink.user_timing,devtools.timeline,disabled-by-default-devtools.timeline,disabled-by-default-devtools.timeline.stack,disabled-by-default-v8.cpu_profiler",
      "args": [
        "--ignore-certificate-errors-spki-list=PhrPvGIaAMmd29hj8BCZOq096yj7uMpRNHpn5PDxI6I=",
        "--user-data-dir=/tmp/google-chrome-wpr"
      ]
    }, 
    "videoParams": {
      "filmstripFullSize": false,
      "thumbsize": 800
    }
  },
  "utc": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty", "coach"]
  },
  "slug": "webpagereplay",
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "annotationRetentionMinutes": 60,
    "addSlugToKey": true,
    "messages": ["pageSummary", "run"]
  },
  "copyLatestFilesToBaseGraphiteNamespace": true,
  "compare": {
    "baselinePath": "/baseline"
  }
}
