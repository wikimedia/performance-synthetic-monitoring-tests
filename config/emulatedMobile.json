{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "3g"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "chrome": {
      "CPUThrottlingRate": 5,
      "collectConsoleLog": true,
      "includeResponseBodies": "html",
      "enableTraceScreenshots": true
    }
  },
  "cpu": true,
  "utc": true,

  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "mobile": true,
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty"]
  }
}
