{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "custom",
      "alias": "100"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "cacheClearRaw": true,
    "chrome": {
      "CPUThrottlingRate": 5,
      "collectConsoleLog": true,
      "includeResponseBodies": "all",
      "enableTraceScreenshots": true,
      "args": ["--ignore-certificate-errors-spki-list=PhrPvGIaAMmd29hj8BCZOq096yj7uMpRNHpn5PDxI6I=","--user-data-dir=~/.config/google-chrome-wpr"]
    }
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 4000,
  "pageCompleteCheckPollTimeout": 1000,
  "timeToSettle": 1000,
  "cpu": true,
  "utc": true,
  "mobile": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty", "coach"]
  },
  "slug": "webpagereplay",
  "graphite": {
    "namespace": "sitespeed_io.emulatedMobile",
    "annotationRetentionMinutes": 10,
    "skipSummary": true,
    "addSlugToKey": true
  }
}
