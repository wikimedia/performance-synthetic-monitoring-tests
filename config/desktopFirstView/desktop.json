{
  "extends": "/config/secrets.json",
  "browsertime": {
    "pageCompleteCheckNetworkIdle": true,
    "timeouts": {
      "pageLoad": 60000,
      "pageCompleteCheck": 60000
    },
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "chrome": {
      "collectConsoleLog": true,
      "enableTraceScreenshots": true,
      "includeResponseBodies": "all",
      "traceCategory": ["disabled-by-default-v8.cpu_profiler"]
    },
    "videoParams": {
      "framerate": 10,
      "thumbsize": 800
    },
    "firefox": {
      "includeResponseBodies": "all",
      "disableBrowsertimeExtension": true,
      "mozillaProPreferences": true
    },
    "viewPort": "1920x1080"
  },
  "cpu": true,
  "utc": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty"]
  },
  "slug": "firstView",
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "annotationRetentionMinutes": 60,
    "skipSummary": true,
    "addSlugToKey": true,
    "messages": ["pageSummary", "run"]
  },
  "copyLatestFilesToBaseGraphiteNamespace": true
}
