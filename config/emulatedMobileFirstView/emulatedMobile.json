{
  "extends": "/config/secrets.json",
  "browsertime": {
    "timeouts": {
      "pageLoad": 60000,
      "pageCompleteCheck": 60000
    },
    "connectivity": {
      "engine": "throttle",
      "profile": "4g"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "videoParams": {
      "framerate": 10,
      "thumbsize": 600
    },
    "chrome": {
      "CPUThrottlingRate": 7,
      "collectConsoleLog": true,
      "includeResponseBodies": "all",
      "enableTraceScreenshots": true
    }
  },
  "cpu": true,
  "utc": true,
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 4000,
  "pageCompleteCheckPollTimeout": 1000,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "mobile": true,
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty"]
  },
  "slug": "firstView",
  "graphite": {
    "namespace": "sitespeed_io.emulatedMobile",
    "annotationRetentionMinutes": 60,
    "skipSummary": true,
    "addSlugToKey": true,
    "messages": ["pageSummary", "run"]
  },
  "copyLatestFilesToBaseGraphiteNamespace": true
}
