{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "custom",
      "alias": "100"
    },
    "visualElements": true,
    "iterations": 5,
    "browser": "chrome",
    "video": false,
    "visualMetric": true,
    "timeToSettle": 5000,
    "delay": 5000,
    "viewPort": "1920x1080",
    "firefox": {
      "includeResponseBodies": "all",
      "disableBrowsertimeExtension": true,
      "mozillaProPreferences": true,
      "acceptInsecureCerts": true,
      "windowRecorder": true
    }
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 5000,
  "pageCompleteCheckPollTimeout": 1000,
  "timeToSettle": 1000,
  "utc": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty", "coach"]
  }
}
