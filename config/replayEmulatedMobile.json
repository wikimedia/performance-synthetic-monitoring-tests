{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "custom",
      "alias": "100"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": false,
    "visualMetric": true,
    "chrome": {
      "CPUThrottlingRate": 5,
      "collectConsoleLog": true,
      "includeResponseBodies": "all",
      "enableTraceScreenshots": true,
      "args": ["--ignore-certificate-errors-spki-list=PhrPvGIaAMmd29hj8BCZOq096yj7uMpRNHpn5PDxI6I=","--user-data-dir=/data/tmp/chrome"]
    }
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 4000,
  "pageCompleteCheckPollTimeout": 1000,
  "timeToSettle": 1000,
  "cpu": true,
  "utc": true,
  "mobile": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "plugins": {
    "remove": ["thirdparty", "coach"]
  }
}
