{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "3g"
    },
    "visualElements": true,
    "iterations": 3,
    "browser": "chrome",
    "video": true,
    "visualMetric": true
  },
  "chrome": {
    "CPUThrottlingRate": 5,
    "collectConsoleLog": true,
    "includeResponseBodies": "html",
    "enableTraceScreenshots": true
  },
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true
  },
  "screenshot": {
    "type": "jpg"
  }
}
