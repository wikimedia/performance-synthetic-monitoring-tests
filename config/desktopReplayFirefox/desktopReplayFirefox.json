{
    "extends": "/config/secrets.json",
    "browsertime": {
        "connectivity": {
            "engine": "external",
            "profile": "custom",
            "alias": "0"
        },
        "visualElements": true,
        "iterations": 21,
        "browser": "firefox",
        "video": false,
        "enableVideoRun": true,
        "visualMetrics": true,
        "cacheClearRaw": true,
        "viewPort": "1920x1080",
        "enableProfileRun": true,
        "firefox": {
            "acceptInsecureCerts": true,
            "bidihar": true,
            "disableBrowsertimeExtension": true
        },
        "videoParams": {
            "filmstripFullSize": false,
            "thumbsize": 800
        }
    },
    "utc": true,
    "gzipHAR": true,
    "html": {
        "fetchHARFiles": true,
        "compareURL": "https://compare.sitespeed.io/"
    },
    "screenshot": {
        "type": "jpg"
    },
    "plugins": {
        "remove": [
            "thirdparty",
            "coach"
        ]
    },
    "slug": "webpagereplay",
    "graphite": {
        "namespace": "sitespeed_io.desktop",
        "annotationRetentionMinutes": 60,
        "addSlugToKey": true,
        "messages": [
            "pageSummary",
            "run"
        ]
    },
    "copyLatestFilesToBaseGraphiteNamespace": true,
    "compare": {
        "baselinePath": "/baseline"
    }
}